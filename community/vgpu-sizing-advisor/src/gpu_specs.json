  "configuration": {
    "hypervisor_reserve_gb": 3.0,
    "safety_buffer_gb": 0.0,
    "default_framework": "vllm",
    "default_quantization": "fp16"
  },
  "workload_overheads": {
    "inference": {
      "multiplier": 1.10,
      "description": "10% overhead for inference workloads (KV cache, activations)"
    },
    "rag": {
      "base_overhead": 0.05,
      "embedding_overhead": 0.03,
      "vector_db_overhead": 0.05,
      "reranker_overhead": 0.02,
      "description": "Dynamic overhead based on RAG components used"
    }
  },
  "framework_overheads": {
    "vllm": {
      "multiplier": 1.25,
      "description": "vLLM with PagedAttention - Efficient memory management"
    },
    "trt-llm": {
      "multiplier": 1.2,
      "description": "TensorRT-LLM - Most efficient inference framework"
    },
    "tgi": {
      "multiplier": 1.35,
      "description": "Text Generation Inference - Standard Hugging Face framework"
    },
    "transformers": {
      "multiplier": 1.5,
      "description": "Hugging Face Transformers - Less optimized for production"
    },
    "sglang": {
      "multiplier": 1.25,
      "description": "SGLang - Similar efficiency to vLLM"
    },
    "llamacpp": {
      "multiplier": 1.3,
      "description": "llama.cpp - Efficient C++ implementation"
    }
  }
